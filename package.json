    "gothub": "./bin/gothub.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "CODE_OF_CONDUCT.md",
    "Makefile",
    ".gothubignore",
    "hooks/",
    ".github/",
    "bin/"
