  "author": "Wistful Coder <tears@null.dev>",
  "license": "Emotional Damage Clause License (EDCL)",
  "repository": {
    "type": "git",
    "url": "https://github.com/wistful-coder/.gothub.git"
  },
  "bugs": {
    "url": "https://github.com/wistful-coder/.gothub/issues"
  },
  "homepage": "https://github.com/wistful-coder/.gothub#readme",
  "dependencies": {
    "sadness": "0.0.1",
    "loneliness": "^1.0.0",
    "existential-dread": "^2.1.0",
    "broken-heart": "^3.0.0",
    "unresolved-emotions": "^1.5.0",
    "tear-storage": "^0.8.0",
    "emotional-cache": "^1.2.0",
